{
  "name": "react-api-cache",
  "version": "1.0.1",
  "description": "React component library to help cache API data, resulting in increased performance and decreased mobile data usage.",
  "main": "./dist/library.js",
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "yarn clean",
    "build": "webpack --mode production && yarn build:example",
    "build:example": "webpack --config example/webpack.config.js --mode production",
    "start": "webpack-dev-server --config example/webpack.config.js --open --mode development",
    "prepublish": "yarn build"
  },
  "keywords": [
    "react",
    "cache",
    "api",
    "performance"
  ],
  "homepage": "https://github.com/caseymorrisus/react-api-cache",
  "repository": {
    "type": "git",
    "url": "git@github.com:caseymorrisus/react-api-cache.git"
  },
  "bugs": {
    "url": "https://github.com/caseymorrisus/react-api-cache/issues"
  },
  "author": "Casey Morris",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.2"
  }
}
